#: main.py:121
msgid "文件名规则 (用于提取ID)"
msgstr "ファイル名規則 (ID抽出用)"

#: main.py:123
#, python-brace-format
msgid "如: {id} 或 {title}▪︎{id}｜{user}等"
msgstr "例: {id} または {title}▪︎{id}｜{user}など"

